public class Manager {
    private int employeeId;
    private String name;
    private String position;

    public Manager(int employeeId, String name, String position) {
        this.employeeId = employeeId;
        this.name = name;
        this.position = position;
    }

    public void approveStock() {
        System.out.println(name + " approved stock.");
    }

    public void generateReport() {
        System.out.println(name + " generated a report.");
    }

    public void monitorInventory() {
        System.out.println(name + " is monitoring inventory.");
    }
}
